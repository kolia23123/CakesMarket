@model CakesMarket.Models.ViewModel.CakesListViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
 
<html>
<head>
    <title>Cake</title>
</head>
<body>
    <table>
        <tr>
            <td>Name</td>
            <td>Category</td>
            <td>Price</td>
            <td></td>
        </tr>
        @foreach (var cake in Model.Cakes)
            {
            <tr>
                <td>@cake.Name</td>
                <td>@cake.Categoty</td>
                <td>@cake.Price</td>
            </tr>
        }
    </table>
</body>
</html>